{
    "compilerOptions": {
        "importHelpers": true,
        "module": "esnext",
        "target": "esnext",
        "outDir": "dist",
        "jsx": "react",
        "strict": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true,
        "plugins": [
            {
                "customTransformers": {
                    "before": ["./transformer/typescript.ts"]
                }
            }
        ]
    },
    "files": ["src2/example.tsx"]
}
